{
  "version": 2,
  "name": "three-js-activities",
  "builds": [
    {
      "src": "Activity 2.1 Lights/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Activity 2.1 Lights/dist"
      }
    },
    {
      "src": "Activity 2.2 Shadows/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Activity 2.2 Shadows/dist"
      }
    },
    {
      "src": "Activity 2.3 Haunted House Sample/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Activity 2.3 Haunted House Sample/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/lights",
      "dest": "/Activity 2.1 Lights/index.html"
    },
    {
      "src": "/shadows",
      "dest": "/Activity 2.2 Shadows/index.html"
    },
    {
      "src": "/haunted-house",
      "dest": "/Activity 2.3 Haunted House Sample/index.html"
    },
    {
      "src": "/",
      "dest": "/Activity 2.3 Haunted House Sample/index.html"
    }
  ]
}
